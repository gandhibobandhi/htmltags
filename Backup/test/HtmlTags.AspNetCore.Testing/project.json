{
  "buildOptions": {
    "warningsAsErrors": true,
    "compile": {
      "include": "../HtmlTags.Testing/**/*.cs",
      "exclude": "../HtmlTags.Testing/obj/**/*.cs"
    },
    "define": [ "ASPNETCORE" ]
  },
  "testRunner": "xunit",
  "dependencies": {
    "HtmlTags.AspNetCore": { "target": "project" },
    "xunit": "2.2.0-beta2-build3300",
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "moq": "4.6.38-alpha"
  },
  "frameworks": {
    "net451": {
      "dependencies": {
        "Microsoft.NETCore.Platforms": "1.0.1"
      }
    },
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        },
        "System.Diagnostics.TraceSource": "4.0.0"
      },
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ]
    }
  }
}
